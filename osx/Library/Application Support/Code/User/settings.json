{
  "workbench.startupEditor": "newUntitledFile",
    "go.inferGopath": true,
    "go.formatTool": "gofmt",
    "go.alternateTools": {
      "gofmt": "golines",
    },
    "editor.tabSize": 2,
    "files.trimTrailingWhitespace": true,
    "explorer.confirmDragAndDrop": false,
    "files.insertFinalNewline": true,
    "[elm]": {
        "editor.formatOnSave": true
    },
    "editor.multiCursorModifier": "ctrlCmd",
    "arduino.additionalUrls": "http://arduino.esp8266.com/stable/package_esp8266com_index.json,https://dl.espressif.com/dl/package_esp32_index.json",
    "editor.minimap.enabled": false,
    "editor.tabCompletion": "on",
    "atomKeymap.promptV3Features": true,
    "editor.formatOnPaste": true,
    "editor.fontSize": 14,
    "editor.detectIndentation": false,
    "[cpp]": {
      "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "platformio-ide.activateOnlyOnPlatformIOProject": true,
    "platformio-ide.disablePIOHomeStartup": true,
    "explorer.confirmDelete": false,
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, IndentWidth: 2, ColumnLimit: 128 }",
    "editor.wordWrapColumn": 120,
    "clangd.path": "/Users/suhlig/Library/Application Support/Code/User/globalStorage/llvm-vs-code-extensions.vscode-clangd/install/10.0.0/clangd_10.0.0/bin/clangd",
    "files.associations": {
      "*.bats": "shellscript"
    },
    "html.format.endWithNewline": true,
    "yaml.schemaStore.enable": false,
    "keyboard.touchbar.enabled": false,
    "hediet.vscode-drawio.local-storage": "",
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
      "**/.classpath": true,
      "**/.project": true,
      "**/.settings": true,
      "**/.factorypath": true
    },
    "markdown.marp.enableHtml": true,
    "go.toolsManagement.autoUpdate": true,
    "shellcheck.enableQuickFix": true,
    "shellcheck.customArgs": [
      "-x"
    ],
    "editor.accessibilitySupport": "off",
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "[python]": {
      "editor.formatOnType": true
    },
    "vale.doNotShowWarningForFileToBeSavedBeforeLinting": true,
    "git.openRepositoryInParentFolders": "always",
    "hediet.vscode-drawio.resizeImages": null,
    "workbench.editor.empty.hint": "hidden",
    "update.showReleaseNotes": false,
    "cmake.showOptionsMovedNotification": false,
    "[rust]": {
      "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "workbench.fontAliasing": "auto",
    "[yaml]": {
      "editor.insertSpaces": true,
      "editor.tabSize": 2,
      "editor.autoIndent": "keep",
      "diffEditor.ignoreTrimWhitespace": false,
      "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
      }
    },
    "go.lintTool": "golangci-lint",
    "editor.fontFamily": "\"IBM Plex Mono\", 'Courier New', monospace"
}
