[user]
	email = nerab@gmx.at
	name = Nicholas E. Rabenau
[credential]
	helper = cache --timeout=86400
[alias]
  co = checkout
  ci = duet-commit
  st = status
  br = branch
	lg = log
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[push]
	default = simple
[core]
	excludesfile = ~/.gitignore
